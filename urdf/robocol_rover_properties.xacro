	<xacro:property name="suspension_effort" value="10.0"/>
	<xacro:property name="suspension_speed" value="10.0"/>
	<xacro:property name="suspension_damping" value="0.5"/>
	<xacro:property name="suspension_friction" value="5.0"/>
	<!-- Wheels properties -->
	<xacro:property name="wheelWidth" value="0.08096"/>
	<xacro:property name="wheelRadius" value="0.1347"/>
	<xacro:property name="wheelMass" value="3.5"/>
	-->
