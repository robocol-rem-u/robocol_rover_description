	<!-- Right Rocker -->
	<xacro:property name="right_rocker_Mass" value="1.5"/>
	<xacro:property name="right_rocker_X" value="0.2794"/>
	<xacro:property name="right_rocker_Y" value="0.5"/>
	<xacro:property name="right_rocker_Z" value="0.5"/>
	<xacro:property name="right_rocker_limit" value="0.4"/>
	<!-- Left Boogie -->
	<xacro:property name="left_boogie_Mass" value="1"/>
	<xacro:property name="left_boogie_X" value="0.2794"/>
	<xacro:property name="left_boogie_Y" value="0.4"/>
	<xacro:property name="left_boogie_Z" value="0.4"/>
	<xacro:property name="left_boogie_limit" value="0.4"/>
	<!-- Right Boogie -->
	<xacro:property name="right_boogie_Mass" value="1"/>
	<xacro:property name="right_boogie_X" value="0.2794"/>
